{
    "manifest_version": 2,
    "name": "Website Locker",
    "description": "For those websites that you spend hours without noticing.",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["library/p5.js", "content/content.js"]
    }],
    "browser_action": {
        "default_title": "Website Locker",
        "default_icon": "images/idea.png",
        "default_popup": "popup/index.html"
    },
    "icons": {
        "16": "images/idea.png",
        "32": "images/idea.png",
        "48": "images/idea.png",
        "128": "images/idea.png"
    },
    "permissions": ["storage"]
}